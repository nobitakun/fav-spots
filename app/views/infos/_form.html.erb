<%= form_for @info do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <div class="form-group">
    <%= f.label :title, 'タイトル' %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :url, 'URL' %>
    <%= f.text_field :url, class: 'form-control' %>
  </div>
  
  <%= f.submit '登録する', class: 'btn btn-primary btn-block' %>
<% end %>